<?xml version="1.0" encoding="UTF-8" ?>
<ContentView
    x:Class="MSO.StimmApp.Views.Attachments.ImageAttachmentView"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <ContentView.Content>
        <Grid>
            <Image Aspect="AspectFill" Source="{Binding AttachmentSource, Converter={StaticResource ImageSourceConverter}}" />
        </Grid>
    </ContentView.Content>
</ContentView>