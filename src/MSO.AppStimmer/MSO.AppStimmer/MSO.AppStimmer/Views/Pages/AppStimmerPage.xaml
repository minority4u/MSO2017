<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="MSO.StimmApp.Views.Pages.AppStimmerPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:appStimmer="clr-namespace:MSO.StimmApp;assembly=MSO.StimmApp"
    xmlns:controls="clr-namespace:MSO.StimmApp.Views.Controls;assembly=MSO.StimmApp"
    Title="Appstimmen"
    BackgroundColor="#F1F4F6"
    BindingContext="{Binding AppStimmer, Source={StaticResource Locator}}">
    <ContentPage.Content>
        <controls:AppStimmerCardView Padding="20" />
    </ContentPage.Content>
</ContentPage>